{
   "css.customData": [
      ".vscode/css_tailwind.json"
   ],
   "css.format.braceStyle": "collapse",
   "css.format.newlineBetweenRules": false,
   "css.format.newlineBetweenSelectors": false,
   "css.validate": true,
   "editor.mouseWheelZoom": true,
   "editor.tabSize": 3,
   "editor.detectIndentation": false,
   "editor.formatOnSave": true,
   "editor.codeActionsOnSave": {
      "source.fixAll": "explicit"
   },
   "eslint.rules.customizations": [
      { "rule": "*", "fixable": true, "severity": "off" }
   ],
   "explorer.fileNesting.enabled": true,
   "explorer.fileNesting.patterns": {
      ".env": ".env.*, telerik-license.*",
      "package.json": "package-lock.json",
      "tsconfig.json": "tsconfig.*, env.d.ts",
      "vite.config.*": "index.html, eslint*, .prettier*"
   },
   "json.format.keepLines": true,
   "html.format.extraLiners": "",
   "html.format.indentInnerHtml": true,
   "tailwindCSS.lint.cssConflict": "ignore",
}